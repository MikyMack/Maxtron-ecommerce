    <script>
        document.addEventListener("DOMContentLoaded", function () {
            const logoutBtn = document.querySelector("a#logout-btn");

            if (logoutBtn) {
                logoutBtn.addEventListener("click", function (event) {
                    event.preventDefault(); // Prevent default link action

                    fetch("/api/auth/user-logout", {
                        method: "POST", // Change to POST if needed
                        credentials: "include" // Ensure cookies are sent
                    })
                        .then(response => {
                            if (response.ok) {
                                window.location.href = "/"; // Redirect after logout
                            } else {
                                console.error("Logout failed");
                            }
                        })
                        .catch(error => console.error("Error:", error));
                });
            }
        });

    </script>
    <script>
        document.querySelectorAll('.qty-btn').forEach(button => {
            button.addEventListener('click', function () {
                const productId = this.dataset.productId;
                const qtyInput = document.getElementById(`qty-${productId}`);
                let qty = parseInt(qtyInput.value);

                if (this.classList.contains('inc')) {
                    qty++;
                } else if (this.classList.contains('dec') && qty > 1) {
                    qty--;
                }
                qtyInput.value = qty;
            });
        });

        document.querySelectorAll('.measurement-select').forEach(select => {
            select.addEventListener('change', function () {
                const productId = this.dataset.productId;
                const selectedIndex = this.value;
                const productsArray = <%- JSON.stringify(products) %>;
                const product = productsArray.find(p => p._id === productId);
                const selectedMeasurement = product.measurements[selectedIndex];

                const discount = selectedMeasurement.offerPrice ? Math.round(((selectedMeasurement.price - selectedMeasurement.offerPrice) / selectedMeasurement.price) * 100) : 0;
                const discountElement = document.querySelector(`#price-${productId}`).previousElementSibling;
                if (discount > 0) {
                    discountElement.textContent = `${discount}% discount`;
                } else {
                    discountElement.textContent = '';
                }

                document.getElementById(`price-${productId}`).textContent = `₹${selectedMeasurement.price}`;
                document.getElementById(`offerPrice-${productId}`).textContent = selectedMeasurement.offerPrice ? `₹${selectedMeasurement.offerPrice}` : `₹${selectedMeasurement.price}`;
            });
        });
    </script>
    <script>
        document.querySelectorAll('.add-to-cart').forEach(button => {
            button.addEventListener('click', async function (e) {
                e.preventDefault();

                const productId = this.dataset.productId;
                const qtyInput = document.getElementById(`qty-${productId}`);
                const quantity = parseInt(qtyInput.value);
                const selectElement = document.getElementById(`measurement-${productId}`);
                const selectedIndex = selectElement.value;
                const productsArray = <%- JSON.stringify(products) %>;
                const product = productsArray.find(p => p._id === productId);
                const selectedMeasurement = product.measurements[selectedIndex];

                if (!selectedMeasurement) {
                    Toastify({
                        text: "Please select a valid size!",
                        duration: 3000,
                        gravity: "top",
                        position: "right",
                        style: { background: "#ff4444" },
                        stopOnFocus: true
                    }).showToast();
                    return;
                }

                const response = await fetch('/add-to-cart', {
                    method: 'PUT',
                    headers: { 'Content-Type': 'application/json' },
                    body: JSON.stringify({
                        productId,
                        length: selectedMeasurement.length,
                        width: selectedMeasurement.width,
                        quantity
                    })
                });

                const data = await response.json();

                if (response.ok) {
                    if (data.isGuest) {
                        // Handle guest user cart addition
                        const cartItems = JSON.parse(localStorage.getItem('guestCart')) || [];
                        const existingItemIndex = cartItems.findIndex(item => item.productId === productId && item.selectedMeasurement.length === selectedMeasurement.length && item.selectedMeasurement.width === selectedMeasurement.width);

                        if (existingItemIndex > -1) {
                            cartItems[existingItemIndex].quantity += quantity;
                        } else {
                            cartItems.push({
                                productId,
                                title: product.title,
                                image: product.images[0],
                                selectedMeasurement: {
                                    length: selectedMeasurement.length,
                                    width: selectedMeasurement.width,
                                    price: selectedMeasurement.price,
                                    offerPrice: selectedMeasurement.offerPrice || 0,
                                },
                                quantity
                            });
                        }

                        localStorage.setItem('guestCart', JSON.stringify(cartItems));
                    }

                    Toastify({
                        text: "Added to cart successfully!",
                        duration: 3000,
                        gravity: "top",
                        position: "right",
                        style: { background: "#28a745" },
                        stopOnFocus: true
                    }).showToast();
                } else {
                    Toastify({
                        text: data.message || "Error adding to cart!",
                        duration: 3000,
                        gravity: "top",
                        position: "right",
                        style: { background: "#ff4444" },
                        stopOnFocus: true
                    }).showToast();
                }
            });
        });
    </script>


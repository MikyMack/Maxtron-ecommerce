                    <script>
                        document.querySelectorAll('.qty-btn').forEach(button => {
                            button.addEventListener('click', function () {
                                const productId = this.dataset.productId;
                                const qtyInput = document.getElementById(`qty-${productId}`);
                                let qty = parseInt(qtyInput.value);

                                if (this.classList.contains('increment')) {
                                    qty++;
                                } else if (this.classList.contains('decrement') && qty > 1) {
                                    qty--;
                                }
                                qtyInput.value = qty;
                            });
                        });

                        document.querySelectorAll('.measurement-select').forEach(select => {
                            select.addEventListener('change', function () {
                                const productId = this.dataset.productId;
                                const selectedIndex = this.value;
                                const product = <%- JSON.stringify(product) %>;
                                const selectedMeasurement = product.measurements[selectedIndex];

                                const discount = selectedMeasurement.offerPrice ? Math.round(((selectedMeasurement.price - selectedMeasurement.offerPrice) / selectedMeasurement.price) * 100) : 0;
                                const discountElement = document.getElementById(`discount-${productId}`);
                                const oldPriceElement = document.getElementById(`old-price-${productId}`);
                                const currentPriceElement = document.getElementById(`current-price-${productId}`);

                                if (oldPriceElement && currentPriceElement) {
                                    if (discount > 0) {
                                        if (discountElement) {
                                            discountElement.textContent = `${discount}% discount`;
                                        }
                                        oldPriceElement.textContent = `₹${selectedMeasurement.price}`;
                                        currentPriceElement.textContent = `₹${selectedMeasurement.offerPrice}`;
                                    } else {
                                        if (discountElement) {
                                            discountElement.textContent = '';
                                        }
                                        oldPriceElement.textContent = '';
                                        currentPriceElement.textContent = `₹${selectedMeasurement.price}`;
                                    }
                                }

                                const lengthElement = document.getElementById(`length-${productId}`);
                                const widthElement = document.getElementById(`width-${productId}`);
                                const priceElement = document.getElementById(`price-${productId}`);
                                const stocksElement = document.getElementById(`stocks-${productId}`);

                                if (lengthElement) lengthElement.textContent = selectedMeasurement.length;
                                if (widthElement) widthElement.textContent = selectedMeasurement.width;
                                if (priceElement) priceElement.textContent = `₹${selectedMeasurement.price}`;
                                if (stocksElement) stocksElement.textContent = selectedMeasurement.stocks;
                            });
                        });

                        document.querySelectorAll('.add-to-cart').forEach(button => {
                            button.addEventListener('click', async function (e) {
                                e.preventDefault();

                                const productId = this.dataset.productId;
                                const qtyInput = document.getElementById(`qty-${productId}`);
                                const quantity = parseInt(qtyInput.value);
                                const selectElement = document.getElementById(`measurement-${productId}`);
                                const selectedIndex = selectElement.value;
                                const product = <%- JSON.stringify(product) %>;
                                const selectedMeasurement = product.measurements[selectedIndex];

                                if (!selectedMeasurement) {
                                    Toastify({
                                        text: "Please select a valid size!",
                                        duration: 3000,
                                        gravity: "top",
                                        position: "right",
                                        style: { background: "#ff4444" },
                                        stopOnFocus: true
                                    }).showToast();
                                    return;
                                }

                                const response = await fetch('/add-to-cart', {
                                    method: 'PUT',
                                    headers: { 'Content-Type': 'application/json' },
                                    body: JSON.stringify({
                                        productId,
                                        length: selectedMeasurement.length,
                                        width: selectedMeasurement.width,
                                        quantity
                                    })
                                });

                                const data = await response.json();

                                if (response.ok) {
                                    if (data.isGuest) {
                                        const cartItems = JSON.parse(localStorage.getItem('guestCart')) || [];
                                        const existingItemIndex = cartItems.findIndex(item => item.productId === productId && item.selectedMeasurement.length === selectedMeasurement.length && item.selectedMeasurement.width === selectedMeasurement.width);

                                        if (existingItemIndex > -1) {
                                            cartItems[existingItemIndex].quantity += quantity;
                                        } else {
                                            cartItems.push({
                                                productId,
                                                title: product.title,
                                                image: product.images[0],
                                                selectedMeasurement: {
                                                    length: selectedMeasurement.length,
                                                    width: selectedMeasurement.width,
                                                    price: selectedMeasurement.price,
                                                    offerPrice: selectedMeasurement.offerPrice || 0,
                                                },
                                                quantity
                                            });
                                        }

                                        localStorage.setItem('guestCart', JSON.stringify(cartItems));
                                    }

                                    Toastify({
                                        text: "Added to cart successfully!",
                                        duration: 3000,
                                        gravity: "top",
                                        position: "right",
                                        style: { background: "#28a745" },
                                        stopOnFocus: true
                                    }).showToast();
                                } else {
                                    Toastify({
                                        text: data.message || "Error adding to cart!",
                                        duration: 3000,
                                        gravity: "top",
                                        position: "right",
                                        style: { background: "#ff4444" },
                                        stopOnFocus: true
                                    }).showToast();
                                }
                            });
                        });
                    </script>
